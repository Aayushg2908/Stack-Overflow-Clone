datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id String @id @default(uuid())
  clerkId String @unique
  name String
  username String @unique
  email String @unique
  picture String

  createdAt DateTime @default(now())
}